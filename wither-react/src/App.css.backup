/* Minecraft-Themed Gaming Platform */
@import url('https://fonts.googleapis.com/css2?family=Rajdhani:wght@400;500;600;700&family=Rubik:wght@400;500;600;700;800&family=Press+Start+2P&display=swap');

* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

body {
    font-family: 'Rubik', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
    background: #1a1a1a;
    color: #a8a8a8;
    min-height: 100vh;
    overflow-x: hidden;
    line-height: 1.6;
    font-size: 15px;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

#background-elements {
    position: fixed;
    inset: 0;
    z-index: -1;
    overflow: hidden;
    pointer-events: none;
    background: 
        radial-gradient(ellipse 800px 600px at 25% 15%, rgba(112, 178, 55, 0.08) 0%, transparent 50%),
        radial-gradient(ellipse 600px 500px at 75% 85%, rgba(133, 79, 43, 0.06) 0%, transparent 50%),
        #1a1a1a;
}

.particle {
    position: absolute;
    border-radius: 50%;
    background: rgba(136, 141, 156, 0.01);
    box-shadow: none;
    animation: float 45s infinite linear;
    opacity: 0.05;
}

@keyframes float {
    0%, 100% { transform: translate3d(0, 0, 0) rotate(0deg); opacity: 0; }
    20%, 80% { opacity: 0.04; }
}

.container {
    max-width: 1440px;
    margin: 0 auto;
    padding: 0 40px;
}

header {
    padding: 18px 0;
    border-bottom: 1px solid rgba(112, 178, 55, 0.15);
    position: sticky;
    top: 0;
    z-index: 1000;
    background: rgba(26, 26, 26, 0.85);
    backdrop-filter: blur(28px) saturate(195%);
    -webkit-backdrop-filter: blur(28px) saturate(195%);
}

.header-content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    max-width: 1440px;
    margin: 0 auto;
    padding: 0 40px;
}

.logo {
    display: flex;
    align-items: center;
    gap: 13px;
    text-decoration: none;
    transition: opacity 0.25s cubic-bezier(0.4, 0, 0.2, 1);
}

.logo:hover { 
    opacity: 0.65; 
}

.logo-img {
    width: 35px;
    height: 35px;
    object-fit: contain;
    filter: brightness(1.08) contrast(1.03);
    border-radius: 7px;
}

.logo h1 {
    font-family: 'Rajdhani', sans-serif;
    font-size: 1.5rem;
    font-weight: 700;
    color: #70B237;
    letter-spacing: 0.02em;
    text-transform: uppercase;
}

.desktop-nav {
    display: flex;
    gap: 3px;
    align-items: center;
}

.nav-btn {
    display: inline-flex;
    align-items: center;
    gap: 8px;
    padding: 9px 16px;
    background: transparent;
    border: none;
    border-radius: 7px;
    color: #868d9c;
    font-family: 'Inter', sans-serif;
    font-weight: 500;
    font-size: 13.5px;
    text-decoration: none;
    cursor: pointer;
    transition: all 0.22s cubic-bezier(0.4, 0, 0.2, 1);
    position: relative;
}

.nav-btn::before {
    content: '';
    position: absolute;
    inset: 0;
    background: linear-gradient(135deg, rgba(88, 81, 219, 0.06), rgba(88, 81, 219, 0.03));
    border-radius: 7px;
    opacity: 0;
    transition: opacity 0.22s;
}

.nav-btn:hover::before {
    opacity: 1;
}

.nav-btn:hover {
    color: #c9ced6;
}

.nav-btn.active {
    color: #ffffff;
    background: linear-gradient(135deg, rgba(88, 81, 219, 0.13), rgba(88, 81, 219, 0.08));
}

.nav-btn i {
    font-size: 13.5px;
    opacity: 0.6;
}

.nav-btn.active i {
    opacity: 0.85;
}

.hamburger {
    display: none;
    flex-direction: column;
    gap: 5px;
    background: transparent;
    border: none;
    cursor: pointer;
    padding: 6px;
}

.hamburger span {
    width: 24px;
    height: 2px;
    background: #868d9c;
    border-radius: 2px;
    transition: all 0.25s cubic-bezier(0.4, 0, 0.2, 1);
}

.hamburger.active span:nth-child(1) { 
    transform: rotate(45deg) translate(6px, 6px); 
}

.hamburger.active span:nth-child(2) { 
    opacity: 0; 
}

.hamburger.active span:nth-child(3) { 
    transform: rotate(-45deg) translate(6px, -6px); 
}

.mobile-menu {
    display: none;
    position: fixed;
    inset: 0;
    background: rgba(7, 8, 10, 0.975);
    backdrop-filter: blur(32px);
    z-index: 999;
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}

.mobile-menu.active {
    opacity: 1;
    visibility: visible;
}

.mobile-menu-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 100%;
    gap: 14px;
    padding: 90px 28px 28px;
}

.mobile-menu-item {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 12px;
    padding: 16px 32px;
    background: rgba(255, 255, 255, 0.025);
    border: 1px solid rgba(255, 255, 255, 0.05);
    border-radius: 11px;
    color: #868d9c;
    font-weight: 500;
    font-size: 15px;
    text-decoration: none;
    width: 90%;
    max-width: 360px;
    transition: all 0.25s cubic-bezier(0.4, 0, 0.2, 1);
    opacity: 0;
    transform: translateY(15px);
    animation: slideIn 0.35s ease-out forwards;
}

.mobile-menu-item:nth-child(1) { animation-delay: 0.06s; }
.mobile-menu-item:nth-child(2) { animation-delay: 0.12s; }
.mobile-menu-item:nth-child(3) { animation-delay: 0.18s; }
.mobile-menu-item:nth-child(4) { animation-delay: 0.24s; }
.mobile-menu-item:nth-child(5) { animation-delay: 0.3s; }
.mobile-menu-item:nth-child(6) { animation-delay: 0.36s; }

@keyframes slideIn {
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

.mobile-menu-item:active {
    background: rgba(255, 255, 255, 0.04);
    transform: scale(0.975);
}

.mobile-menu-item.active {
    background: linear-gradient(135deg, rgba(88, 81, 219, 0.13), rgba(88, 81, 219, 0.08));
    border-color: rgba(88, 81, 219, 0.18);
    color: #ffffff;
}

.mobile-menu-item i {
    font-size: 17px;
    opacity: 0.55;
}

.mobile-menu-item.active i {
    opacity: 0.8;
}

.hero {
    padding: 100px 0 70px;
    text-align: center;
}

.hero h2 {
    font-family: 'Space Grotesk', sans-serif;
    font-size: 4rem;
    font-weight: 700;
    margin-bottom: 22px;
    color: #ffffff;
    letter-spacing: -0.04em;
    line-height: 1.08;
}

.hero p {
    font-size: 1.125rem;
    max-width: 720px;
    margin: 0 auto 48px;
    color: #9299a6;
    font-weight: 450;
    line-height: 1.7;
}

.server-info {
    position: fixed;
    bottom: 28px;
    right: 28px;
    z-index: 998;
    display: flex;
    flex-direction: column;
    gap: 12px;
}

.server-ip {
    background: rgba(13, 15, 19, 0.94);
    border: 1px solid rgba(255, 255, 255, 0.055);
    border-radius: 12px;
    padding: 14px 18px;
    transition: all 0.25s cubic-bezier(0.4, 0, 0.2, 1);
    backdrop-filter: blur(32px);
    cursor: pointer;
    box-shadow: 
        0 8px 32px rgba(0, 0, 0, 0.48),
        inset 0 1px 0 rgba(255, 255, 255, 0.02);
}

.server-ip:hover {
    border-color: rgba(88, 81, 219, 0.22);
    transform: translateY(-3px);
    box-shadow: 
        0 12px 40px rgba(0, 0, 0, 0.56),
        0 0 0 1px rgba(88, 81, 219, 0.1),
        inset 0 1px 0 rgba(255, 255, 255, 0.03);
}

.ip-label {
    font-size: 10.5px;
    color: #5e6678;
    font-weight: 600;
    letter-spacing: 1px;
    margin-bottom: 5px;
    text-transform: uppercase;
}

.ip-address {
    font-family: 'JetBrains Mono', 'Courier New', monospace;
    font-size: 13.5px;
    font-weight: 600;
    color: #e2e5ea;
    margin-bottom: 10px;
    letter-spacing: -0.01em;
}

.copy-btn {
    display: inline-flex;
    align-items: center;
    gap: 6px;
    background: linear-gradient(135deg, #5851db 0%, #6c63ff 100%);
    color: #ffffff;
    border: none;
    padding: 6px 13px;
    border-radius: 6px;
    font-weight: 600;
    font-size: 12px;
    cursor: pointer;
    transition: all 0.2s cubic-bezier(0.4, 0, 0.2, 1);
    box-shadow: 0 2px 8px rgba(88, 81, 219, 0.25);
}

.copy-btn:hover {
    background: linear-gradient(135deg, #4a43c4 0%, #5a51e6 100%);
    transform: translateY(-1px);
    box-shadow: 0 4px 12px rgba(88, 81, 219, 0.35);
}

.copy-btn.copied {
    background: linear-gradient(135deg, #0ea57a 0%, #10b981 100%);
    box-shadow: 0 2px 8px rgba(16, 185, 129, 0.25);
}

.player-count {
    display: flex;
    align-items: center;
    gap: 8px;
    background: rgba(13, 15, 19, 0.94);
    border: 1px solid rgba(255, 255, 255, 0.055);
    border-radius: 10px;
    padding: 10px 16px;
    backdrop-filter: blur(32px);
    font-size: 13px;
    color: #9299a6;
    font-weight: 500;
    box-shadow: 
        0 8px 32px rgba(0, 0, 0, 0.48),
        inset 0 1px 0 rgba(255, 255, 255, 0.02);
}

.player-count i {
    color: #5851db;
    font-size: 14px;
}

.cta-buttons {
    display: flex;
    justify-content: center;
    gap: 16px;
    flex-wrap: wrap;
}

.btn {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    text-decoration: none;
    font-weight: 600;
    padding: 14px 32px;
    border-radius: 9px;
    transition: all 0.22s cubic-bezier(0.4, 0, 0.2, 1);
    font-size: 14.5px;
    letter-spacing: -0.015em;
}

.btn.primary {
    background: linear-gradient(135deg, #5851db 0%, #6c63ff 100%);
    color: #ffffff;
    box-shadow: 0 4px 16px rgba(88, 81, 219, 0.28);
}

.btn.primary:hover {
    background: linear-gradient(135deg, #4a43c4 0%, #5a51e6 100%);
    transform: translateY(-2px);
    box-shadow: 0 6px 24px rgba(88, 81, 219, 0.38);
}

.btn.secondary {
    background: transparent;
    color: #9299a6;
    border: 1.5px solid rgba(255, 255, 255, 0.08);
}

.btn.secondary:hover {
    border-color: rgba(255, 255, 255, 0.15);
    background: rgba(255, 255, 255, 0.025);
    color: #c9ced6;
}

@media (max-width: 768px) {
    .desktop-nav { display: none !important; }
    .hamburger { display: flex !important; }
    .mobile-menu { display: block !important; }
    .container { padding: 0 24px; }
    .header-content { padding: 0 24px; }
    .hero { padding: 70px 0 50px; }
    .hero h2 { font-size: 2.5rem; }
    .hero p { font-size: 1rem; }
    .server-info {
        bottom: 20px;
        right: 20px;
        left: 20px;
    }
}

.feature-card:hover {
    transform: translateY(-10px);
    border: 1px solid rgba(59, 130, 246, 0.3);
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
}

.feature-icon {
    font-size: 2.5rem;
    margin-bottom: 20px;
    color: #3b82f6;
}

.feature-card h3 {
    font-size: 1.4rem;
    margin-bottom: 15px;
    color: #ffffff;
}

.feature-card p {
    color: #94a3b8;
    font-size: 1rem;
}

/* Game Modes Section */
.game-modes {
    padding: 80px 0;
    text-align: center;
}

.game-modes h2 {
    font-family: 'Orbitron', sans-serif;
    font-size: 2.2rem;
    font-weight: 700;
    margin-bottom: 60px;
    background: linear-gradient(45deg, #3b82f6, #06b6d4);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
}

.modes-container {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 30px;
}

.mode-card {
    background: rgba(15, 23, 42, 0.5);
    border: 1px solid rgba(255, 255, 255, 0.05);
    border-radius: 15px;
    padding: 30px 25px;
    transition: all 0.4s ease;
    text-align: left;
    backdrop-filter: blur(10px);
}

.mode-card:hover {
    transform: translateY(-10px);
    border: 1px solid rgba(59, 130, 246, 0.3);
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
}

.mode-header {
    display: flex;
    align-items: center;
    gap: 15px;
    margin-bottom: 20px;
}

.mode-icon {
    font-size: 1.8rem;
    color: #3b82f6;
}

.mode-card h3 {
    font-size: 1.5rem;
    color: #ffffff;
}

.mode-status {
    display: inline-block;
    padding: 5px 15px;
    border-radius: 20px;
    font-weight: 700;
    margin-bottom: 20px;
    text-transform: uppercase;
    font-size: 0.8rem;
    letter-spacing: 1px;
}

.released {
    background: linear-gradient(45deg, #00b09b, #96c93d);
    color: #000000;
}

.coming-soon {
    background: linear-gradient(45deg, #ff9900, #ffcc00);
    color: #000000;
}

.mode-card p {
    color: #94a3b8;
    margin-bottom: 20px;
    font-size: 1rem;
}

.mode-features {
    list-style: none;
}

.mode-features li {
    padding: 5px 0;
    color: #94a3b8;
    position: relative;
    padding-left: 20px;
}

.mode-features li::before {
    content: '•';
    color: #3b82f6;
    position: absolute;
    left: 0;
}

/* Stats section */
.stats { position: relative; overflow: hidden;
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 20px;
    padding: 60px 0;
}

.stat-card {
    background: rgba(15, 23, 42, 0.5);
    border: 1px solid rgba(255, 255, 255, 0.05);
    border-radius: 15px;
    padding: 30px 20px;
    text-align: center;
    backdrop-filter: blur(10px);
}

.stat-number {
    font-family: 'Orbitron', sans-serif;
    font-size: 2rem;
    font-weight: 700;
    color: #3b82f6;
    margin-bottom: 10px;
}

.stat-label {
    color: #94a3b8;
    font-size: 1rem;
}

/* Footer */
footer {
    padding: 60px 0 30px;
    border-top: 1px solid rgba(255, 255, 255, 0.05);
}

.footer-content {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 40px;
    margin-bottom: 40px;
}

.footer-section h3 {
    font-size: 1.3rem;
    margin-bottom: 20px;
    color: #ffffff;
}

.footer-section p {
    color: #94a3b8;
    margin-bottom: 20px;
    font-size: 0.95rem;
}

.social-links {
    display: flex;
    gap: 15px;
}

.social-links a {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.05);
    color: #94a3b8;
    transition: all 0.3s ease;
}

.social-links a:hover {
    background: #3b82f6;
    color: white;
    transform: translateY(-3px);
}

.footer-section ul {
    list-style: none;
}

.footer-section ul li {
    margin-bottom: 12px;
}

.footer-section ul li a {
    text-decoration: none;
    color: #94a3b8;
    transition: all 0.3s ease;
    font-size: 0.95rem;
}

.footer-section ul li a:hover {
    color: #3b82f6;
    padding-left: 5px;
}

.footer-bottom {
    text-align: center;
    padding-top: 30px;
    border-top: 1px solid rgba(255, 255, 255, 0.05);
    color: #707090;
    font-size: 0.9rem;
}

.footer-bottom p {
    margin: 8px 0;
}

.made-with-love {
    color: #94a3b8;
    font-weight: 500;
    margin-top: 10px;
}

.made-with-love::before {
    animation: heartbeat 1.5s ease-in-out infinite;
}

@keyframes heartbeat {
    0%, 100% {
        transform: scale(1);
    }
    10%, 30% {
        transform: scale(1.1);
    }
    20%, 40% {
        transform: scale(1);
    }
}

/* Testimonials Section */
.testimonials { position: relative; overflow: hidden;
    padding: 80px 0;
    text-align: center;
}

.testimonials h2 {
    font-family: 'Orbitron', sans-serif;
    font-size: 2.2rem;
    font-weight: 700;
    margin-bottom: 60px;
    background: linear-gradient(45deg, #3b82f6, #06b6d4);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
}

.testimonials-container {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 30px;
}

.testimonial-card {
    background: rgba(15, 23, 42, 0.5);
    border: 1px solid rgba(255, 255, 255, 0.05);
    border-radius: 15px;
    padding: 30px 25px;
    transition: all 0.4s ease;
    text-align: left;
    backdrop-filter: blur(10px);
}

.testimonial-card:hover {
    transform: translateY(-10px);
    border: 1px solid rgba(59, 130, 246, 0.3);
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
}

.testimonial-content p {
    color: #94a3b8;
    font-style: italic;
    margin-bottom: 20px;
    line-height: 1.7;
}

.testimonial-author {
    display: flex;
    align-items: center;
    gap: 15px;
}

.author-avatar {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    background: linear-gradient(45deg, #3b82f6, #06b6d4);
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 1.2rem;
}

.author-info h4 {
    color: #ffffff;
    margin-bottom: 5px;
}

.author-role {
    color: #64748b;
    font-size: 0.9rem;
}

/* FAQ Section */
.faq { position: relative; overflow: hidden;
    padding: 80px 0;
    text-align: center;
}

.faq h2 {
    font-family: 'Orbitron', sans-serif;
    font-size: 2.2rem;
    font-weight: 700;
    margin-bottom: 60px;
    background: linear-gradient(45deg, #3b82f6, #06b6d4);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
}

.faq-container {
    max-width: 800px;
    margin: 0 auto;
    text-align: left;
}

.faq-item {
    background: rgba(15, 23, 42, 0.5);
    border: 1px solid rgba(255, 255, 255, 0.05);
    border-radius: 15px;
    margin-bottom: 20px;
    overflow: hidden;
    backdrop-filter: blur(10px);
}

.faq-question {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20px 25px;
    cursor: pointer;
    transition: all 0.3s ease;
}

.faq-question:hover {
    background: rgba(59, 130, 246, 0.1);
}

.faq-question h3 {
    color: #ffffff;
    font-size: 1.2rem;
    margin: 0;
}

.faq-question i {
    color: #3b82f6;
    transition: transform 0.3s ease;
}

.faq-item.active .faq-question i {
    transform: rotate(180deg);
}

.faq-answer {
    padding: 0 25px;
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease, padding 0.3s ease;
}

.faq-item.active .faq-answer {
    padding: 0 25px 25px;
    max-height: 200px;
}

.faq-answer p {
    color: #94a3b8;
    line-height: 1.7;
}

/* Responsive design */
@media (max-width: 992px) {
    .hero h2 {
        font-size: 2.3rem;
    }
    
    .hero p {
        font-size: 1.1rem;
    }
    
    .ip-address {
        font-size: 1.6rem;
    }
    
    .desktop-nav {
        gap: 12px;
    }
    
    .nav-btn {
        padding: 8px 14px;
        font-size: 0.8rem;
        gap: 6px;
    }
    
    .nav-btn i {
        font-size: 0.9rem;
    }
}

@media (max-width: 768px) {
    .desktop-nav {
        display: none !important;
    }
    
    .hamburger {
        display: flex !important;
    }
    
    .mobile-menu {
        display: block !important;
    }
    
    .logo h1 {
        font-size: 1.5rem;
    }
    
    .hero {
        padding: 40px 0 100px;
    }
    
    .hero h2 {
        font-size: 2rem;
    }
    
    .hero p {
        font-size: 1rem;
    }
    
    .server-info {
        position: fixed;
        bottom: 20px;
        right: 20px;
        left: 20px;
        flex-direction: column;
        gap: 10px;
    }
    
    .server-ip {
        padding: 15px 20px;
        width: 100%;
    }
    
    .ip-address {
        font-size: 1rem;
    }
    
    .copy-btn {
        padding: 6px 14px;
        font-size: 0.8rem;
    }
    
    .player-count {
        padding: 8px 16px;
        font-size: 0.85rem;
        width: 100%;
        justify-content: center;
    }
    
    .features {
        padding: 60px 0;
    }
    
    .game-modes {
        padding: 60px 0;
    }
    
    .stats {
        grid-template-columns: repeat(2, 1fr);
    }
}

@media (max-width: 480px) {
    .container {
        padding: 0 15px;
